.card-box.is_valid view {
  color: #cdcdcd;
}

.card-box.is_valid text {
  color: #cdcdcd;
}

.card-box .coupon-item {
  background-color: #fff;
  border: solid 1rpx #e0e0e0;
  margin: 20rpx 20rpx 0 20rpx;
}

.card-box .info-box {
  display: flex;
  align-items: center;
  height: 200rpx;
  padding: 20rpx;
}

.card-box .active {
  background-color: #fff;
  border: solid 1rpx #00a7f2;
}

.card-box .info-rule {
  margin: 5rpx 20rpx;
  padding: 20rpx;
  background-color: #fff;
  border-top: solid 1rpx #e0e0e0;
}

.info-box .f-coupon {
  height: 200rpx;
  line-height: 180rpx;
  flex: 1;
  margin: 0 20rpx;
}

.info-box .f-coupon-detail {
  position: relative;
  height: 200rpx;
  /* line-height:100px; */
  flex: 4;
}

.card-box .info-box .content .title {
  display: flex;
  align-items: center;
  height: 96rpx;
  line-height: 32rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.card-box .info-box .content .time {
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: middle;
}

.split-line {
  border-left: 1px dashed #eee;
  margin: 0 20px 0 0px;
  background: #fff;
  height: 180rpx;
  width: 1px;
  float: left;
}

.rule {
  position: absolute;
  bottom: -10rpx;
  right: 0;
}

.select-img {
  width: 30px;
  height: 30px;
  position: absolute;
  right: 0;
  bottom: 80rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 30px 30px;
}

.select-img.active {
  border: none;
  background-image: url("data:image/png;base64,");
}

.no-data {
  text-align: center;
  padding-top: 100rpx;
}

.no-data image {
  display: inline-block;
  width: 150rpx;
  height: 150rpx;
}

.no-data .text {
  margin-top: 20rpx;
}
