@import '../../style/weui.wxss';

.message{
  text-align: center;
  width:750rpx;
}

.message-time{
  margin-top: 20rpx;
  height:40rxp;
  width:160rpx;
  margin-left: auto; 
  margin-right: auto;
  background-color: #b5bcc4;
  color: white;
  border-radius:4rpx;
}

.message-content{
  margin-top: 20rpx;
  background-color: white;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.message-content .top{
  width:100%;
  height: 80rpx;
  border-bottom: solid 1rpx #e0e0e0;
}

.message-content .top .icon{
  float: left;
  margin-left: 30rpx;
  margin-top: 20rpx;
  width:40rpx;
  height:40rpx;
}

.message-content .top .title{
  float: left;
  margin-left: 20rpx;
  height:80rpx;
  line-height:80rpx;
}

.message-content .middle{
  text-align: left;
  width:100%;
  border-bottom: solid 1rpx #e0e0e0;
}

.message-content .middle .title{
  text-align: left;
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-top: 30rpx;
}

.message-content .middle .content{
  text-align: left;
  margin: 30rpx;
  line-height: 42rpx;
}

.message-content .bottom{
  width:100%;
}

.message-content .bottom .title{
  text-align: left;
  margin-left: 30rpx;
  margin-right: 30rpx;
  line-height: 80rpx;
}

.message-content .bottom .weui-select{
  padding-left:0 ;
  padding-right:0;
  border-right: 0;
}

.notices { margin: 20rpx 30rpx; }
.notices .notice { padding: 20rpx 30rpx; background: #fff; margin-top: 20rpx; border-radius: 10rpx; }
.notices .notice .title { color: #333; }
.notices .notice .date { color: #808892; margin-top: 6rpx; }
.notices .notice .content { color: #808892; margin-top: 20rpx; display: block }

.no-data { padding-top: 150rpx; color:#808892; text-align: center; }
.no-data .no-data-icon { width: 70rpx; height: 70rpx; margin-left: auto; margin-right: auto; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100% 100%; }
.no-data .no-data-text { margin-top: 10rpx; }

.nomore { text-align: center; line-height: 80rpx; color: #999; padding-bottom: 20rpx; }

.f-big { font-size: 32rpx; }
.f-title { font-size: 32rpx; }
.f-normal { font-size: 28rpx; }
.f-small { font-size: 24rpx; }