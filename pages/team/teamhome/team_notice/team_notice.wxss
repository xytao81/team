.notice { overflow: hidden; background: #fff; padding: 0 30rpx; line-height: 80rpx; border-radius: 50rpx; margin-left: 20rpx; margin-right: 20rpx; }
.notice .icon { float: left; width: 40rpx; height: 40rpx; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100% 100%; margin-top: 20rpx; }
.notice .text { float: left; margin-left: 16rpx; line-height: 80rpx; color: #808892; }
.notice .close { float: right; width: 30rpx; height: 30rpx; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100% 100%; margin-top: 26rpx; }