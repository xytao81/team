    padding:0rpx 30rpx 0 30rpx;
  }
  .abstract-img{
    width:100%;
  }
  .footer-buttons .back-btn .is-checked{ background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100% 100%; }
  .footer-buttons .back-btn .normal{ background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100% 100%; }

  .footer-buttons .back-btn .icon-poster{ background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100% 100%; }
  

/* 弹出框 */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 90;
  opacity: 0.7;
}
.modalDlg{
  width:100%;
  height:100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
  /* margin: -370rpx 85rpx; */
  /* background-color:#171E22; */
  background-color:rgba(23,30,34,0.3);
  /* opacity:0.7; */
  /* border-radius: 36rpx; */
  display: flex;
  flex-direction: column;
  align-items: center;
}
.close{
  width:100%;
  /* border:1rpx solid red; */
}
.close > image{
  width:50rpx;
  height:50rpx;
  padding-left:30rpx;
  padding-top:30rpx;
}
.poster-img{
  opacity:1;
  width:680rpx;
  margin:60rpx 30rpx 90rpx 30rpx;
  text-align: center;
}
.poster-img > image{
  width:80%;
  height:700rpx;
}
.down{
  display: flex;
  width:90%;
}
.weixin,.friend,.photo{
  flex:30%;
  text-align: center;
  /* border:1rpx solid red; */
}
.weixin-img,.friend-img,.photo-img{
  width:70rpx;
  height:70rpx;
 
}
.shell{
  color:#fff;
  font-size:20rpx;
}
button.weixin,button.friend{
  background-color:rgba(23,30,34,0);
  line-height:41rpx
}
.friend{
  display:none;
}
button.weixin::after{ border: none; }
button.friend::after{ border: none; }
/* btns */
