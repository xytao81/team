/* 已报名/已签到 */

.page-section {
  margin-bottom: 20rpx;
}
.f-gray{
  display:flex;
}
.examine,.agree,.refuse{
  text-align:center;
  flex:33.33%;
  color:#333;
}
.me { background: #ecf7fc; }

.player-row {
   padding-left: 30rpx; 
   border: 0; 
   font-size: 32rpx; 
   line-height: 44rpx; 
   padding-top: 30rpx; 
   padding-bottom: 30rpx; 
   overflow: hidden; 
   height: 120rpx; 
   border-bottom: 1rpx solid #e0e0e0; 
}
.player-row .avatar { 
  text-align: center; 
  width: 84rpx; 
  height: 120rpx; 
  position: relative; 
}
.player-row .avatar .pos { 
  position: absolute; 
  left: 0; bottom: 0; 
  width: 100%; 
  height: 36rpx; 
  line-height: 36rpx; 
  font-size: 26rpx; 
  background: rgba(0,0,0,0.3); 
  color: #fff; 
}
.player-row .right { 
  width: 550rpx; 
  min-height: 120rpx; 
  font-size: 28rpx; 
}
.player-row .right .t1 { 
  margin-top: 2rpx; 
  color: #333; 
  overflow: hidden; 
}
.player-row .right .t2 { 
  margin-top: 16rpx; 
}

.player-row .badge { 
  width: auto; 
  font-size: 24rpx; 
  margin-top: 2rpx; 
}
.player-row .badge .c0 { 
  background: #00a7f2; 
}
.player-row .badge .c1 { 
  background: rgb(150,200,58); 
}
.player-row .badge .c2 { 
  background: #888; 
}
.player-row .badge .c3 { 
  background: #bcbcbd; 
}

.weui-badge { 
  border-radius: 6rpx; 
}
.to-examine{
  color: #00a7f2; 
}

.signin-btn {
  font-size:12px;
  width:105rpx;
  margin-right:30rpx;
  margin-top:30rpx;
  height:50rpx;
  line-height: 55rpx;
}



