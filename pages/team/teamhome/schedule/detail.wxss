    position: relative;
    /* margin-bottom: 8rpx; */
  }
  .detail-img{
    height:422rpx;
    width:100%;
  }
  .video-remind{
     position: absolute;
     bottom: 0;
     right:0;
     background:#00a7f2;
     opacity: 0.8; 
     border-radius:4rpx;
     color:#fff;
     width:240rpx;
     height:60rpx;
     text-align: center;
     line-height: 60rpx;
  }
  
  /* 中间介绍 */
  .detail-desc{
    padding:20rpx 0 20rpx 0;
    background-color:#fff;
  }
  .detail-title{
    color:#333;
    padding:0 30rpx;
    line-height:60rpx;
    font-weight:bold;

  }
  .detail-palnning,.detail-palnning-map{
    display:flex;
    /* border:1rpx solid red; */
    height:40rpx;
    padding-top:10rpx;
  }

  .detail-palnning-left{
    width:120rpx;
    padding:0 0rpx 0 30rpx;
    color:#333;
  }
  .total-palnning-right{
    flex:1;
    padding-right:30rpx;
    margin-left:30rpx;
    color:#808892;
  }
  .detail-palnning-right{
    flex:1;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    padding-right:30rpx;
  }
  .detail-palnning-right2{
    flex:1;
    overflow:hidden;
    padding-right:30rpx;
    padding-left:30rpx;
    color:#808892;
  }
  .detail-palnning-right,.detail-palnning-local> text{
    padding-left:30rpx;
    color:#808892;
  }
  .gonggao-palnning-local{
    flex:1;
    padding-right:30rpx;
    color:#808892;
    margin-left:30rpx;

  }
  .match-palnning-local{
    flex:1;
    padding-right:30rpx;
    color:#808892;
    margin-left:30rpx;
  }
  .detail-palnning-local{
   
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    padding-right:30rpx;
  }
  .detail-palnning-img{
    padding:0 30rpx;
  }
  .detail-palnning-img image{
    height:40rpx;
    width:40rpx;
    line-height: 40rpx;
  }

  

  /* 活动简介 */
  .abstract{
    background:#fff;
    margin-top:20rpx;
    font-weigth:bold;
    margin-bottom:20rpx;
    /* padding-bottom:20rpx; */
  }
