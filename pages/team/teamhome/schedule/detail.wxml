                <view class="mt o-h">
                    <view class="f-l score-label">正赛比分</view>
                    <view class="f-r score-right o-h">
                        <view class="f-l r r1"><input type="number" class="score-input" value="{{ scheduleScoreForm.homeTeamScore }}" data-key="homeTeamScore" bindinput="onScoreChange" /></view>
                        <view class="f-l r r2">:</view>
                        <view class="f-l r r3"><input type="number" class="score-input" value="{{ scheduleScoreForm.awayTeamScore }}" data-key="awayTeamScore" bindinput="onScoreChange" /></view>
                    </view>
                </view>
                <view class="mt o-h">
                    <view class="f-l score-label">点球决胜比分</view>
                    <view class="f-r score-right o-h">
                        <view class="f-l r r1"><input type="number" class="score-input" value="{{ scheduleScoreForm.homeTeamPointScore }}" data-key="homeTeamPointScore" bindinput="onScoreChange" /></view>
                        <view class="f-l r r2">:</view>
                        <view class="f-l r r3"><input type="number" class="score-input" value="{{ scheduleScoreForm.awayTeamPointScore }}" data-key="awayTeamPointScore" bindinput="onScoreChange" /></view>
                    </view>
                </view>
                <view class="mt">
                    <button class="score-button" type="primary" bindtap="saveScore">保存</button>
                </view>
            </view>
        </supermodal>

        <supermodal title="直播详情" show="{{ liveModalVisible }}" showButtons="{{ false }}" wx:if="{{ teamRights > 0 && isAdmin && schedule.live_id }}">
            <view class="f-normal" style="padding: 30rpx;">
                <view>
                    <view class="" style="word-break:break-all;"><viewimage class="detail-img" src="{{ schedule.live.qrcode_url }}"></viewimage></view>     
                    <view class="t-c" style="color: #808892;">直播二维码</view>
                </view>
            </view>
