
<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
    <view class="page__bd" style="padding-bottom: 140rpx;">
        <view wx:if="{{ schedule.live_id }}">
            <video id="myVideo" src="{{ schedule.live.url || schedule.live.play_url }}" controls style="width:750rpx;height:422rpx;" wx:if="{{ (schedule.live.status == 2 || schedule.live.status == 5) || isAdmin }}"></video>
            <view class="detail lh-0" wx:else>
                <image class="detail-img" src="{{ schedule.banner_url ? schedule.banner_url : 'https://aqm-photo.oss-cn-hangzhou.aliyuncs.com/static/schedule/default_banner.png' }}?x-oss-process=image/resize,m_fill,h_211,w_375"></image>
                <view class="video-remind f-normal" wx:if="{{ schedule.live.status == 1 }}">直播未开始</view>
                <view class="video-remind f-normal" wx:if="{{ schedule.live.status == 3 || schedule.live.status == 6 }}">直播已结束</view>
                <view class="video-remind f-normal" wx:if="{{ schedule.live.status == 4 }}">直播已暂停</view>
            </view>
        </view>
        <view wx:else>
            <!-- 判断是哪种详情（活动，训练，比赛） -->
            <view class="detail lh-0" wx:if="{{ schedule.type == 1 }}">
                <viewimage class="detail-img" src="{{ schedule.banner_url ? schedule.banner_url : 'https://aqm-photo.oss-cn-hangzhou.aliyuncs.com/static/schedule/default_banner.png' }}?x-oss-process=image/resize,m_fill,h_450,w_800"></viewimage>
            </view>
        </view>
        <view class="match-desc" wx:if="{{ schedule.type == 4 }}">
            <view class="match-desc-title f-title">{{schedule.match.league_info.name}}</view>
            <view class="match-desc-info">{{schedule.match.league_info.zone ? schedule.match.league_info.zone :'全国'}}<text>{{schedule.match.league_info.players}} </text><text>{{schedule.match.league_info.type_name}} </text></view>
        </view>
        <view class="detail" wx:if="{{ schedule.type == 4 }}">
            <view class="show-container" style="padding-left:30rpx;padding-right:30rpx;">
                <view class="show-row" style="border-bottom: none;">
                    <view class="show-title">
                        <view class="f-l tit">
                            <text class="title-name">本队赛程</text> 
                        </view>
                        <view class="f-r">
                            <!-- <navigator url="/pages/my/info/edit?group=person_info" style="line-height: 0;margin-top:20rpx;"> -->
                            <navigator url="/pages/league/leagueitem/index_h5?id={{schedule.match.league_id}}&team_id={{schedule.team_id}}" style="line-height: 0;margin-top:20rpx;">
                                <button class="weui-btn" type="primary" size="mini" plain="true" class="show-morebtn" style="line-height: 50rpx;margin-top:0rpx;">查看赛事数据</button>
                            </navigator>
                        </view>
                    </view>
                </view>
            </view>
            <view class="schedule-item">
                <view class="schedule-item-info">
                    <view class="f2">
                        <view class="match-flexbox f-black">
                            <view class="home-box" wx:if="{{ schedule.match }}">
                                <image class="team-logo" src="{{ schedule.match.home.logo ? schedule.match.home.logo : '../../../../images/league/team_logo.png'}}"></image>
                                <text class="team-name f-blue" wx:if="{{ schedule.is_home }}">{{ schedule.match.home.name }}</text>    
                                <text class="team-name" wx:if="{{ !schedule.is_home }}">{{ schedule.match.home.name }}</text>    
                            </view>
                            <view class="vs-box">
                                <view class="img-box-left o-h">
                                    <view class="img-box-left2">
                                        <view class="shirts shirts-left clothos-container lh-0"  style=" background:url('{{schedule.match.home.clothes.player.shirt.color}}') center -10rpx  no-repeat; width:30rpx; height:30rpx;" wx:if="{{ schedule.match.home.clothes.player.shirt.id != 0 }}">
                                            <image class="clothos" src="../../../../images/yifu.png" style="width: 30rpx; height: 30rpx;"></image>     
                                        </view>
                                    </view>
                                </view>
                                <view class="name-box o-h">
                                    <view wx:if="{{ schedule.match.result }}" style="display:inline-block">
                                        <view class="status-box f-score">{{schedule.match.result.home_score}}:{{schedule.match.result.away_score}}</view>
                                        <view class="f-small f-gray" wx:if="{{schedule.match.result.home_point_score && schedule.match.result.away_point_score}}">点球[{{schedule.match.result.home_point_score}}:{{schedule.match.result.away_point_score}}]</view>
                                        <!-- <view class="f-small f-gray">点球[{{schedule.match.result.home_score}}:{{schedule.match.result.away_score}}]</view> -->
                                    </view>
                                    <view class="vs f-score" style="width:auto;height:100rpx;line-height:100rpx;" wx:else>VS</view>
                                </view>
                                <view class="img-box-right o-h">
                                    <view class="img-box-right2">
                                        <view class="shirts shirts-right clothos-container lh-0"  style=" background:url('{{schedule.match.away.clothes.player.shirt.color}}') center -10rpx  no-repeat; width:30rpx; height:30rpx;" wx:if="{{ schedule.match.away.clothes.player.shirt.id != 0 }}">
                                            <image class="clothos" src="../../../../images/yifu.png" style="width: 30rpx; height: 30rpx;"></image>     
                                        </view>
                                    </view>
                                </view>
                            </view>

                            <view class="away-box" wx:if="{{ schedule.match }}">
                                <image class="team-logo" src="{{ schedule.match.away.logo ? schedule.match.away.logo : '../../../../images/league/team_logo.png'}}"></image>
                                <text class="team-name" wx:if="{{ schedule.is_home }}">{{ schedule.match.away.name }}</text>    
                                <text class="team-name f-blue" wx:if="{{ !schedule.is_home }}">{{ schedule.match.away.name }}</text>    
                            </view>
                        </view>
                    </view>
                </view>
            </view>   
        <!-- 活动 -->
        <!-- <image class="detail-img" src="{{ schedule.banner_url ? schedule.banner_url : 'https://aqm-photo.oss-cn-hangzhou.aliyuncs.com/static/schedule/default_banner.png' }}?x-oss-process=image/resize,m_fill,h_211,w_375"></image> -->
        <!-- <view class="video-remind">直播已结束</view> -->
        </view>
        <view class="detail" wx:if="{{ schedule.type == 2 }}">
            <view class="schedule-item">
                <view class="schedule-item-info">
                    <view class="f2">
                        <view class="match-flexbox">
                            <view class="match-flexbox">
                                <view class="home-box" wx:if="{{ schedule.is_home }}">
                                    <image class="team-logo" src="{{ team.logo ? team.logo : 'https://aqm-photo.oss-cn-hangzhou.aliyuncs.com/static/default_photo.png'}}"></image>
                                    <text class="team-name f-blue">{{ team.name }}</text>
                                </view>
                                <view class="home-box" wx:if="{{ !schedule.is_home }}">
                                    <image class="team-logo" src="../../../../images/league/team_logo.png"></image>
                                    <text class="team-name">{{ schedule.target_team_name }}</text>
                                </view>
                            </view>
                            <view class="match-flexbox">
                                <view class="vs-box">
                                    <view class="vs t-c " style="{{ isAdmin ? 'margin-top:0;' : ''}}" wx:if="{{ (schedule.home_team_score!=''&&schedule.home_team_score!=null) || (schedule.away_team_score!=''&&schedule.away_team_score!=null) || (schedule.home_team_point_score!=''&&schedule.home_team_point_score!=null) || (schedule.away_team_point_score!=''&&schedule.away_team_point_score!=null) }}">
                                        {{schedule.home_team_score||0}}:{{schedule.away_team_score||0}}
                                        <view class="f-small f-gray" style="display:block;" wx:if="{{ (schedule.home_team_point_score!=''&&schedule.home_team_point_score!=null) || (schedule.away_team_point_score!=''&&schedule.away_team_point_score!=null) }}">点球[{{schedule.home_team_point_score||0}}:{{schedule.away_team_point_score||0}}]</view>
                                    </view>
                                    <view class="vs" wx:else>VS</view>
                                    <view class="f-small" style="display:block;" wx:if="{{ isAdmin }}" bindtap="openScoreModal">
                                        <button class="vs-button" type="primary" bindtap="loginSubmit">录入比分</button>
                                    </view>
                                </view>
                                <view class="status-box"></view>
                            </view>
                            <view class="match-flexbox">
                                <view class="away-box" wx:if="{{ schedule.is_home }}">
                                    <image class="team-logo" src="../../../../images/league/team_logo.png"></image>
                                    <text class="team-name">{{ schedule.target_team_name }}</text>
                                </view>
                                <view class="away-box" wx:if="{{ !schedule.is_home }}">
                                    <image class="team-logo" src="{{ team.logo ? team.logo : 'https://aqm-photo.oss-cn-hangzhou.aliyuncs.com/static/default_photo.png'}}"></image>
                                    <text class="team-name f-blue">{{ team.name }}</text>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>   
        <!-- 活动 -->
        </view>
    <!-- 中间介绍 -->
        <view class="detail-desc f-normal">
            <!-- 这个标题也是根据类型判断有没有（比赛没有，其他有） -->
            <view class="detail-title f-title" wx:if="{{ schedule.type !=2 }}">{{ schedule.title }}</view>
            <view class="detail-palnning">
                <view class="detail-palnning-left">
                    <text wx:if="{{ schedule.type == 1 }}">活动时间</text>
                    <text wx:if="{{ schedule.type == 2 }}">比赛时间</text>
                    <text wx:if="{{ schedule.type == 3 }}">训练时间</text>
                    <text wx:if="{{ schedule.type == 4 }}">比赛时间</text>
                </view>
                <view class="detail-palnning-right" wx:if="{{schedule.type == 4}}">
                    <text>{{ schedule.match.start_time }}</text>
                </view>
                <view wx:else class="detail-palnning-right" >
                    <text>{{ schedule.start_time }}</text>
                </view>       
            </view>
            <view class="detail-palnning" wx:if="{{ schedule.city_name }}">
                <view class="detail-palnning-left">
                    <text>地区</text>
                </view>
                <view class="detail-palnning-right">
                    <text>{{ schedule.province_name }}-{{ schedule.city_name }}-{{ schedule.district_name }}</text>
                </view>     
            </view>
            <view class="gonggao" wx:if="{{ schedule.type == 4 && schedule.match.place.title }}">
                <view class="detail-palnning-left">
                    <text>比赛地点</text>
                </view>
                <view class="match-palnning-local">
                    <text>{{ schedule.match.place.title}}</text>
                </view>

            </view>
            <view class="gonggao" wx:if="{{ schedule.place }}">
                <view class="detail-palnning-left">
                    <text wx:if="{{ schedule.type == 1 }}">活动地点</text>
                    <text wx:if="{{ schedule.type == 2 }}">比赛地点</text>
                    <text wx:if="{{ schedule.type == 3 }}">训练地点</text>
                </view>
                <view class="total-palnning-right">
                    <text>{{ schedule.place }}</text>
                </view>

            </view>
            
            <view class="gonggao" wx:if="{{ schedule.type == 4 && schedule.match.desc}}">
                <view class="detail-palnning-left">
                    <text>公告</text>
                </view>
                <view class="gonggao-palnning-local">
                    <text>{{schedule.match.desc}}</text>
                </view>

            </view>
                <!-- 本队球衣 -->
            <view class="detail-palnning-map" wx:if="{{ schedule.type == 4 && colors.clothes.player.shirt.id != 0 }}">
                <view class="detail-palnning-left">
                    <text>本队球衣</text>
                </view>
                <view class="detail-palnning-local">
                    <text class="person">球员</text>
                    <view class="clothos-container" style="background: url({{colors.clothes.player.shirt.color}}) center -10rpx  no-repeat;">
                        <image class="clothos" src="../../../../images/icon_sy.png"></image>     
                    </view>
                    <view class="clothos-container" style="background: url({{colors.clothes.player.shorts.color}}) center -10rpx  no-repeat;">
                        <image class="clothos" src="../../../../images/icon_kz.png"></image>     
                    </view>
                    <view class="clothos-container" style="background: url({{colors.clothes.player.shoes.color}}) center -10rpx  no-repeat;">
                        <image class="clothos" src="../../../../images/icon_qw.png"></image>     
                    </view>
 
                    <text class=" person-right">守门员</text>
                    <view class="clothos-container" style="background: url({{colors.clothes.goalkeeper.shirt.color}}) center -10rpx  no-repeat;">
                        <image class="clothos" src="../../../../images/icon_sy.png"></image>     
                    </view>
                    <view class="clothos-container" style="background: url({{colors.clothes.goalkeeper.shorts.color}}) center -10rpx  no-repeat;">
                        <image class="clothos" src="../../../../images/icon_kz.png"></image>     
                    </view>
                    <view class="clothos-container" style="background: url({{colors.clothes.goalkeeper.shoes.color}}) center -10rpx  no-repeat;">
                        <image class="clothos" src="../../../../images/icon_qw.png"></image>     
                    </view>
                </view>
            </view>

            <view class="detail-palnning-map" wx:if="{{ schedule.address }}">
                <view class="detail-palnning-left">
                    <text>导航地址</text>
                </view>
                <view class="detail-palnning-right o-h">
                    <text>{{ schedule.address }}</text>
                </view>
                <view class="detail-palnning-img" wx:if="{{ schedule.place_lat }}">              
                    <image bindtap="listenerBtnGetLocation" src="data:image/png;base64,"></image>
                </view>
            </view>
           
            <view class="detail-palnning" wx:if="{{ schedule.sponsor }}">
                <view class="detail-palnning-left">
                    <text>主办方</text>
                </view>
                <view class="detail-palnning-right">
                    <text>{{ schedule.sponsor }}</text>
                </view>     
            </view>
            <view class="detail-palnning" wx:if="{{ schedule.is_enroll && schedule.enroll_start_time && schedule.enroll_end_time }}">
                <view class="detail-palnning-left">
                    <text>报名时间</text>
                </view>
                <view class="detail-palnning-right">
                <text>{{ schedule.show_enroll_start_time }} 至 {{ schedule.show_enroll_end_time }}</text>
                </view>     
            </view>
            <view class="detail-palnning" wx:if="{{ schedule.players_max > 0 && schedule.is_enroll}}">
                <view class="detail-palnning-left">
                    <text>报名人数</text>
                </view>
                <view class="detail-palnning-right">
                <text>{{ schedule.players_max }}</text>
                </view>     
            </view>
        
        </view>
       <!--活动简介  -->
        <view class="abstract">
            <view class="abstract-tilte f-title" wx:if="{{ schedule.type == 1 }}">活动简介</view>
            <view class="abstract-tilte f-title" wx:if="{{ schedule.type == 2 }}">比赛要求</view>
            <view class="abstract-tilte f-title" wx:if="{{ schedule.type == 3 }}">训练要求</view>
            <view class="abstract-tilte f-title" wx:if="{{ schedule.type == 4 }}">参赛要求</view>
            <!-- <view class="abstract-contant">{{ schedule.intro }}</view> -->
            <!-- 查看完整简介 -->
            <!-- <view wx:if="{{ !expanded }}" class="abstract-contant">{{ shortDesc }}</view>
            <view wx:if="{{ expanded }}" class="abstract-contant">{{ desc }}</view> -->
            <view class="abstract-contant f-normal">{{ desc }}</view>
            <!-- <view class="f-small t-c f-blue" style="line-height: 80rpx; " wx:if="{{ !expanded }}" bindtap="showDesc">查看完整介绍</view>
            <view class="f-small t-c f-blue" style="line-height: 80rpx; " wx:if="{{ expanded }}" bindtap="hideDesc">收起介绍</view> -->

            <view class="abstract-img-list {{ schedule.introImagesArray.length >= 2 && imgExpanded ? 'isShow' : '' }}" style="{{schedule.introImagesArray.length ==1 ? 'max-height:100%' : 'max-height:440rpx;'}};">
                <image class="abstract-img" mode="widthFix" src="{{ item }}?x-oss-process=image/resize,w_375" wx:for="{{ schedule.introImagesArray }}" data-src="{{ item }}" wx:key="id" bindtap="previewAll"></image>
            </view>
            <view class="f-small t-c f-blue" style="line-height: 80rpx; " wx:if="{{ schedule.introImagesArray.length >= 2 && !imgExpanded }}" bindtap="showImg">展开详情</view>
            <view class="f-small t-c f-blue" style="line-height: 80rpx; " wx:if="{{ schedule.introImagesArray.length >= 2 && imgExpanded }}" bindtap="hideImg">收起详情</view>
        </view>
     <!-- 已报名/已签到 -->

        <view class="f-normal f-gray" wx:if="{{ enrollButtonStatus != 2 && enrollButtonStatus != 4 }}" style="border: 0; line-height: 44rpx; padding-top: 30rpx; padding-bottom: 30rpx;overflow: hidden; height: 30rpx; line-height: 30rpx; background: #fff;" >
            <view class="'tabs-item examine' + {{state==0?'to-examine':''}} f-normal" bindtap='onTabsItemTab' data-status=''>报名表</view>
            <view class="'tabs-item agree' + {{state==1?'to-examine':''}} f-normal" bindtap='onTabsItemTab'  data-status='1'>签到表</view>   
        </view>
        <!-- 报名签到的统计 -->
        <view class="count"  wx:if="{{ enrollButtonStatus != 2 && enrollButtonStatus != 4 }}" >
            <text class="count-examine" wx-if="{{state==0}}">已报名 {{enrollCount.all }} 人</text>
            <text class="count-agree" wx-if="{{state==0}}">已同意 {{ enrollCount.agreed }} 人</text>
            <text class="count-info" wx-if="{{state==1}}">已签到 {{ enrollCount.arrived }} 人</text>
            <text class="count-leave" wx-if="{{state==1}}">已请假 {{ enrollCount.vacated }} 人</text>
        </view>
    
        <view class="players-detail" wx:if="{{ enrollButtonStatus != 2 && enrollButtonStatus != 4 }}">
            <!-- 待审核 -->
            <users list='{{ enrolls }}' isAdmin="{{ false }}" showSignedStatus="{{ state == 1 ? true : false }}" bindclickuser="onClickUser"></users>
        </view>

     <!-- 编辑 取消 报名管理-->
        <view class="mt-30 btns" wx:if="{{ teamRights > 0 && isAdmin }}">
            <button class="btns-group" bindtap="gotoEdit" style="margin-left:0;">
                <image class="btn-edit" src="data:image/png;base64,"></image>
                <view class="btn-text f-normal">编辑活动</view>
            </button>
            <button style="margin-left:0;" class="btns-group" bindtap="confirmCancelSchedule" wx:if="{{ schedule.type != 4 }}">
                <image class="" src="data:image/png;base64,"></image>
                <view class="btn-text f-normal">取消活动</view>
            </button>
            <button class="btns-group" wx:if="{{ schedule.type == 4 }}" style="margin-left:0;">
                <image class="" src="../../../../images/quxiao.png"></image>
                <view class="btn-text f-normal btn-gray">取消活动</view>
            </button>
            <button class="btns-group" bindtap="generatePoster">
                <!-- <view class="icon icon-poster"></view> -->
                <image src="data:image/png;base64,"></image>
                <view class="haibao f-normal" style="color:#333;">生成海报</view>
            </button>
            <button style="margin-left:0;" class="btns-group" bindtap="gotoUsers" style="position:relative" wx:if="{{ schedule.is_enroll }}">
                <image class="" src="data:image/png;base64,"></image>
                <view class="btn-text f-normal">报名管理</view>
                <view class="weui-badge weui-badge_dot" style="position: absolute;top:5rpx;right:40rpx;" wx:if="{{ schedule.status_count.waiting > 0 }}"></view>
            </button>
            <button style="margin-left:0;" class="btns-group" style="position:relative" wx:if="{{ !schedule.is_enroll }}">
                <image class="" src="data:image/png;base64,"></image>
                <view class="btn-text f-normal btn-gray">报名管理</view>
            </button>
        </view>
        <view class="mt-30 btns" wx:if="{{ teamRights > 0 && isAdmin && schedule.live_id }}">
            <button style="margin-left:0;" class="btns-group" bindtap="gotoLiveControl">
                <image class="btn-edit" src="../../../../images/zhibo.png"></image>
                <view class="btn-text f-normal">直播管理</view>
            </button>
        </view>

        <view class="footer-buttons" wx:if="{{ !isAdmin }}">
        <!-- <view class="back-btn" bindtap="generatePoster">
            <view class="icon icon-poster"></view>
            <view class="text">生成海报</view>
        </view> -->
        <!--报名按钮状态 -->
        <block wx:if="{{ enrollButtonStatus == 6 }}">
            <button class="full-btn gray-btn" type="primary" wx:if="{{ enrollButtonStatus == 6 }}">此活动已取消</button>
        </block>
        <block wx:elif="{{ enroll }}">
            <!-- 待审核按钮状态 -->
            <block wx:if="{{ enroll.status == 0 }}">
                <button class=" full-btn cancle" plain="true" style="display:inline-block;width:30%;" type="primary" bindtap="cancelEnrollConfirm">取消报名</button>
                <button class="full-btn audit" plain="true" style="display:inline-block; width:70%;" type="primary" bindtap="gotoEnrollEdit">待审核 ，修改资料</button>
            </block>
            <!-- 请假按钮状态 -->
            <block wx:if="{{ enroll.status == 1 }}">
                <button class=" full-btn cancle" plain="true" style="display:inline-block;width:30%;" type="primary" bindtap="leaveConfirm" wx:if="{{ enroll.arrive == 0 }}">请假</button>
                <button class=" full-btn cancle" plain="true" style="display:inline-block;width:30%;" type="primary" wx:if="{{ enroll.arrive == 1 }}">已签到</button>
                <button class=" full-btn cancle" plain="true" style="display:inline-block;width:30%;" type="primary" wx:if="{{ enroll.arrive == 2 }}">已请假</button>
                <button class="full-btn audit" plain="true" style="display:inline-block; width:70%;" type="primary" bindtap="gotoEnroll">报名已通过</button>
            </block>
            <!-- 未通过按钮状态 -->
            <block wx:if="{{ enroll.status == 2 }}">
                <button class="full-btn" type="primary" bindtap="gotoEnroll">审核未通过,重新报名</button>
            </block>
        </block>
        <block wx:else>
        <block wx:if="{{ enrollButtonStatus == 1 }}">
            <button class="full-btn" type="primary" wx:if="{{ !enroll & schedule.type != 4}}" bindtap="gotoEnrollEdit">报名</button>
            <button class="full-btn" type="primary" wx:if="{{ !enroll & schedule.type == 4}}" bindtap="showinfo">报名</button> 
        </block>
        <button class="full-btn gray-btn" type="primary" wx:if="{{ enrollButtonStatus == 2 }}">报名未开始</button>
        <button class="full-btn gray-btn" type="primary" wx:if="{{ enrollButtonStatus == 3 }}">报名已结束</button>
        <button class="full-btn gray-btn" type="primary" wx:if="{{ enrollButtonStatus == 4 }}">无需报名</button>
        <button class="full-btn gray-btn" type="primary" wx:if="{{ enrollButtonStatus == 5 }}">已报满</button>
        </block>
        
        </view>

    
     <!-- 弹框 -->
        <view class="mask" catchtouchmove="preventTouchMove" wx:if="{{showPoster}}"></view>
        <view class="reason modalDlg" wx:if="{{showPoster}}" title="">
            <view class="close" bindtap="hidePoster">
                <image src="data:image/png;base64,"></image>
            </view>
            <view class="poster-img shadow" bindtap="cancel">
                <image mode="widthFix" src="{{ posterUrl }}"></image>
            </view>
            <view class="down">
                <button class="weixin"  open-type="share">
                    <image class="weixin-img" src="data:image/png;base64,"></image>
                    <view class="shell">分享给好友</view>
                </button>
                <button class="friend">
                    <image class="friend-img" src="data:image/png;base64,"></image>
                    <view class="shell">分享至朋友圈</view>
                </button>
                <view class="photo" bindtap="downloadFile">
                    <image class="photo-img" src="data:image/png;base64,"></image>
                    <view class="shell">保存至相册</view>
                </view>
            </view>
        </view>

        <supermodal title="录入比分" show="{{ scoreModalVisible }}" bindcancel="saveScoreCancel" showButtons="{{ false }}">
            <view class="score-modal f-normal">
                <!-- <view class="o-h">
                    <view class="f-l"></view>
                    <view class="f-r score-right o-h">
                        <view class="f-l r r1">主队</view>
                        <view class="f-l r r2">VS</view>
